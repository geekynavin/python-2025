class Car:
    def __init__(self, brand, speed, auto):
        self.brand = brand
        self.speed = speed
        self.auto = auto

    def show(self):
        print(f"Brand: {self.brand}\nSpeed: {self.speed} km/h\nAutomatic: {self.auto}")
    

# Create object
c1 = Car("Toyota", 120,"Yes")
c1.show()
